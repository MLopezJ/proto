{
  "name": "@nrf.guide/proto",
  "version": "0.0.0-development",
  "description": "Documents the communication protocol between the nrf.guide backend and the web application",
  "type": "module",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./export.js",
      "node": "./export.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/nRF.guide-proto.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/nRF.guide-proto/issues"
  },
  "homepage": "https://nrf.guide",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "nrf-guide"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "prepublish": "npx swc -C jsc.experimental.keepImportAssertions=true -d dist ./validator"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.13",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "13.0.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.44",
    "@swc/jest": "0.2.24",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "chalk": "5.2.0",
    "eslint": "8.37.0",
    "glob": "9.3.2",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "prettier-plugin-organize-imports": "3.2.2",
    "tsx": "3.12.6",
    "typescript": "5.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "LICENSE",
    "README.md",
    "example.ts",
    "export.js",
    "index.d.ts",
    "dist",
    "nrfcloud-application-protocols/schemas"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/nrfcloud-application-protocols/"
    ]
  },
  "dependencies": {
    "ajv": "8.12.0"
  }
}
